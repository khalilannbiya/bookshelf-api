{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "eslint:recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": ["error", "windows"],
    "no-multiple-empty-lines": ["error", { "max": 3 }],
    "import/extensions": ["error", "always", { "ignorePackages": true }],
    "import/prefer-default-export": ["off"]
  }
}
